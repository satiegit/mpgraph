\newlength{\kbcircleboxwdht}
\newcommand{\kbcentcirc}[1]{%
  \setlength{\kbcircleboxwdht}{\wd\kbcirclebox}%
  \addtolength{\kbcircleboxwdht}{\dp\kbcirclebox}%
%  \raisebox{0.4\dp\kbcirclebox}{%
  \raisebox{0.1\dp\kbcirclebox}{%
%    \parbox[][\kbcircleboxwdht][c]{\wd\kbcirclebox}{\centering#1}}%
%    \ifnum \value{#1}>9
    \parbox[][\kbcircleboxwdht][c]{\wd\kbcirclebox}{\centering\scriptsize#1}%    
%    \else
%    \parbox[][\kbcircleboxwdht][c]{\wd\kbcirclebox}{\centering\footnotesize#1}%
%    \fi
  }%
  \llap{\usebox{\kbcirclebox}}%
}
